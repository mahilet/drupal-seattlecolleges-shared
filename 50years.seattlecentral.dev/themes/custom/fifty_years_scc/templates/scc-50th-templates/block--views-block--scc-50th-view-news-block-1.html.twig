{% set classes = [
'block',
'block-' ~ configuration.provider|clean_class,
'block-' ~ plugin_id|clean_class,
'clearfix', 'container-fluid', 'bg-yellow' ,'sc-negative-space'
] %}
<section {{ attributes.addClass(classes) }}>
    <div class="container">
        <header>
            <h2 class="sc-title-heading">{{ content['#title']['#markup'] }}</h2>
        </header>
        <div class="row card-container">
            {% block content %}
                {{ content }}
            {% endblock %}
        </div>
    </div>
</section>